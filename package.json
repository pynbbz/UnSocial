{
  "name": "unsocial",
  "version": "1.0.0",
  "description": "Social media to RSS Feed converter - Windows Desktop App",
  "main": "src/main.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pynbbz/UnSocial"
  },
  "keywords": ["rss", "social-media", "instagram", "twitter", "facebook", "linkedin", "electron", "feed", "scraper"],
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder --win"
  },
  "build": {
    "appId": "com.unsocial.app",
    "productName": "UnSocial",
    "win": {
      "target": "portable",
      "icon": "assets/icon.png"
    },
    "portable": {
      "artifactName": "UnSocial.exe",
      "requestExecutionLevel": "user",
      "unpackDirName": "UnSocial-data"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "feed": "^4.2.2"
  },
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1"
  }
}
